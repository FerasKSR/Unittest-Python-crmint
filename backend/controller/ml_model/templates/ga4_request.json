{
  "clientId": "${client_id}",
  {% if unique_id == 'user_id' %}
  "userId": "${user_id}",
  {% endif %}
  "nonPersonalizedAds": false,
  "events": [
    {
      "name": "${event_name}",
      "params": {
        "type": "${type}",
        "value": "${value}",
        {% if label.is_score %}
        "score": "${score}",
        "nscore": "${normalized_score}"
        {% elif label.is_revenue %}
        "revenue": "${revenue}"
        {% endif %}
      }
    }
  ]
}
